 <div class="row">
   <div>
    <label>Algorithm:</label> <%=contestant.algorithm.name%> - <%= contestant.algorithm.description%>
  </div>
  <div style="padding-left:5px">
    <h5>metrics:</h5>
    <% contestant.metrics.each do |k,v|%>
      <div>
        <span style="font-size:1em;"><%=k%>:</span> <%= "%.3f" % v   %>
      </div>
    <%end%>
  </div>
</div>